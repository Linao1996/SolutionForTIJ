package pac2;

import java.util.*;

public class Controller {
	static private LinkedList<Integer> integers = new LinkedList<Integer>();

	private static ListIterator<Integer> it ;

	public static void main(String[] args) {
		for (int i = 0; i < 10; i++) {
			it = integers.listIterator(integers.size() / 2);
			it.add(i);
		}
		System.out.println(integers);
	}

}
