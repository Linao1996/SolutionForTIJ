package pac2;

public class getEx {
	static String[] s = { "a", "b", "c" };

	public static void main(String[] args) {
		try {
			System.out.println(s[3]);
		} catch (ArrayIndexOutOfBoundsException a) {
			System.out.println(a);
			System.out.println(s[2]);
		} finally {
			System.out.println("reached finally");
		}

	}

}
