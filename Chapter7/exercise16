package pac1;
class Amphibian{
	public String toString(){
		return "Amphibian";
	}
	static void name(Amphibian amp){
		System.out.println(amp);
	}
}

public class Frog extends Amphibian{
	public String toString(){
		return "Frog";
	}
	public static void main(String [] args){
		Frog frog = new Frog();
		Amphibian amp  = new Amphibian();
		Amphibian.name(frog);
		Amphibian.name(amp);
	}
}
